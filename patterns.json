{
    "patterns": [
      {
        "name": "Credit Card Numbers",
        "regex": "(?:\\d[ -]*?){13,16}"
      },
      {
        "name": "Social Security Numbers (US)",
        "regex": "\\b\\d{3}-\\d{2}-\\d{4}\\b"
      },
      {
        "name": "IPv4 Addresses",
        "regex": "\\b(?:\\d{1,3}\\.){3}\\d{1,3}\\b"
      },
      {
        "name": "IPv6 Addresses",
        "regex": "\\b(?:[A-Fa-f0-9]{1,4}:){7}[A-Fa-f0-9]{1,4}\\b"
      },
      {
        "name": "Bitcoin and Ethereum Wallet Addresses",
        "regex": "\\b[13][a-km-zA-HJ-NP-Z1-9]{25,34}\\b"
      },
      {
        "name": "MAC Addresses",
        "regex": "\\b([0-9A-Fa-f]{2}[:-]){5}([0-9A-Fa-f]{2})\\b"
      },
      {
        "name": "Windows File Paths",
        "regex": "[A-Za-z]:\\\\(?:[A-Za-z0-9_-]+\\\\)*[A-Za-z0-9_-]+(?:\\.[A-Za-z0-9]+)?"
      }
    ]
  }
  